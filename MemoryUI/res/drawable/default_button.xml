<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_window_focused="false" android:state_enabled="true"
        android:drawable="@drawable/default_button_normal_light" />
    <item android:state_window_focused="false" android:state_enabled="false"
        android:drawable="@drawable/default_button_disabled_light" />
    <item android:state_pressed="true"
        android:drawable="@drawable/default_button_pressed_light" />
    <item android:state_focused="true" android:state_enabled="true"
        android:drawable="@drawable/default_button_focused_light" />
    <item android:state_enabled="true"
        android:drawable="@drawable/default_button_normal_light" />
    <item android:state_focused="true"
        android:drawable="@drawable/default_button_disabled_focused_light" />
    <item
         android:drawable="@drawable/default_button_disabled_light" />
</selector>
